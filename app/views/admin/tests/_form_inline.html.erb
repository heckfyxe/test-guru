<%= form_with model: [:admin, resource],
              url: update_inline_admin_test_path(resource),
              local: true,
              class: 'form-inline hide',
              data: { test_id: resource.id } do |form| %>
  <%= form.text_field :title, class: 'form-control mr-2' %>
  <%= form.submit 'submit', class: 'btn btn-primary' %>
<% end %>